{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "frontend",
  "sourceRoot": "apps/frontend",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt dev",
        "cwd": "apps/frontend"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt build",
        "cwd": "apps/frontend"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt start",
        "cwd": "apps/frontend"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt preview",
        "cwd": "apps/frontend"
      }
    }
  },
  "tags": ["frontend", "nuxt"]
}
